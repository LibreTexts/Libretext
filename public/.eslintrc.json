{
    "root": true,
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": [
        "airbnb",
        "plugin:jsdoc/recommended"
    ],
    "globals": {
        "LibreTexts": "readonly",
        "LibreEditor": "writable",
        "CKEDITOR": "writable",
        "getCC": "readonly",
        "tippy": "readonly",
        "Popper": "readonly"
    },
    "ignorePatterns": [
        "*.min.js"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "jsdoc"
    ],
    "rules": {
        "no-alert": "off",
        "no-console": "off",
        "react/function-component-definition": [2, {
            "namedComponents": "arrow-function",
            "unnamedComponents": "arrow-function"
        }]
    }
}
